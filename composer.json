        "php": "^7.3|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/support": "^7.0.5|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "orchestra/testbench": "^6.4",
        "nunomaduro/collision": "^5.1"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Albin Nilsson",
            "email": "albin@infab.io"
        },
        {
            "name": "Daniel KÃ¤llstrand Modig",
            "email": "daniel@ikoncept.se"
        }
    ],
    "autoload": {
        "psr-4": {
            "Infab\\TranslatableRevisions\\": "src/",
            "Infab\\TranslatableRevisions\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {"Infab\\TranslatableRevisions\\Tests\\": "tests"}
